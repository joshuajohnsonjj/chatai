        "jsonwebtoken": "^9.0.2",
        "jwks-rsa": "^3.1.0",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "nest-winston": "^1.9.4",
        "node-forge": "^1.3.1",
        "openai": "^4.47.1",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.2.2",
        "stripe": "^15.1.0",
        "uuid": "^9.0.1",
        "winston": "^3.13.0",
        "winston-cloudwatch": "^6.2.0"
