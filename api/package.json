        "@joshuajohnsonjj38/mongodb": "^1.0.12",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.0.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.3.8",
        "@prisma/client": "5.12.1",
        "axios": "^1.6.8",
        "class-transformer": "^0.5.1",
